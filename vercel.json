{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/v1/chat/completions",
        "dest": "/app.js",
        "methods": ["POST"]
      },
      {
        "src": "/",
        "dest": "/app.js",
        "methods": ["GET"]
      }
    ]
  }